<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <View
        android:id="@+id/ellipse_2"
        android:layout_width="127dp"
        android:layout_height="127dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="-27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-22dp"
        android:backgroundTint="#8F00BFA6"
        android:background="@drawable/custom_circle"
        />
    <View
        android:id="@+id/ellipse_3"
        android:layout_width="182dp"
        android:layout_height="187dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="113dp"
        android:backgroundTint="#B4F9EC"
        android:layout_marginEnd="-70dp"
        android:background="@drawable/custom_circle"
        />
    <View
        android:id="@+id/ellipse_4"
        android:layout_width="122dp"
        android:layout_height="122dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="-40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:backgroundTint="#FFFFFF"
        android:background="@drawable/custom_circle"
        />
    <View
        android:id="@+id/ellipse_1"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-58dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-54dp"
        android:backgroundTint="#943BE3CD"
        android:background="@drawable/custom_circle"
        />

    <TextView
        android:id="@+id/g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="132dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="-8dp"
        android:text="G"
        android:textAppearance="@style/G_styleTitle"
        android:fontFamily="@font/baloo"
        android:gravity="top"
        />

    <TextView
        android:id="@+id/pharmacy_lo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="205dp"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/baloo"
        android:gravity="top"
        android:text="Pharmacy"
        android:textAppearance="@style/pharmacieTitle" />

    <TextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="208dp"
        android:layout_marginTop="68dp"
        android:fontFamily="@font/baloo"
        android:gravity="top"
        android:text="Location"
        android:textAppearance="@style/pharmacieTitle" />

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="435dp"
        android:layout_below="@id/location"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        android:alpha="0.7"
        android:background="@drawable/custom_button" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:padding="10dp"
        android:gravity="center"
        android:background="#3BE3CD">
        <View
            android:id="@+id/home_badge"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/home"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            />
        <View
            android:id="@+id/search_badge"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/search"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            />
        <View
            android:id="@+id/list_badge"
            android:layout_width="54dp"
            android:layout_height="54dp"
            android:background="@drawable/list"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            />
    </LinearLayout>
</RelativeLayout>